{
  "name": "@instello/dashboard",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm with-env next dev --turbopack",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.24.0",
    "@hookform/resolvers": "catalog:",
    "@instello/api": "workspace:*",
    "@instello/db": "workspace:*",
    "@instello/ui": "workspace:*",
    "@phosphor-icons/react": "catalog:",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.83.0",
    "@trpc/client": "^11.4.3",
    "@trpc/tanstack-react-query": "^11.4.3",
    "dotenv-cli": "^8.0.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.4",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-hook-form": "catalog:",
    "superjson": "^2.2.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@instello/eslint-config": "workspace:*",
    "@instello/prettier-config": "workspace:*",
    "@instello/tsconfig": "workspace:*",
    "@types/node": "catalog:react19",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@instello/prettier-config"
}
